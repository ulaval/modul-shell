{
    "name": "@ulaval/shell-ui",
    "version": "0.1.4",
    "description": "Javascript lib to help bootstrap a multi-modules application.",
    "author": "Frédéric Poliquin <frederic.poliquin@dti.ulaval.ca>",
    "private": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/ulaval/shell-ui.git"
    },
    "scripts": {
        "build": "rimraf dist/**/* && npm run tslint && tsc",
        "buildWebpack": "rimraf dist/**/* && webpack --config conf/webpack.config.js --color",
        "clean": "rimraf dist",
        "declarations": "node conf/declarations.js",
        "dev": "webpack-dev-server --config conf/webpack.config.js --content-base src --colors --port 5000 --history-api-fallback",
        "prepack": "npm run build",
        "prod": "http-server dist -p 5000 -g -o",
        "unit": "",
        "e2e": "",
        "setup": "npm install --no-optional",
        "test": "npm run unit && npm run e2e",
        "tslint": "tslint -c conf/tslint.json -e 'src/**/*.d.ts' -e 'tests/unit/**/*.d.ts' -s node_modules/custom-tslint-formatters/formatters -t grouped 'src/**/*.ts' 'tests/unit/**/*.ts' 'tests/e2e/*.ts'"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/node": "^7.0.46",
        "@types/es6-promise": "^0.0.33",
        "@types/google.analytics": "^0.0.36",
        "awesome-typescript-loader": "^3.3.0",
        "dts-generator": "^2.1.0",
        "html-webpack-plugin": "^2.30.1",
        "http-server": "^0.10.0",
        "parallel-webpack": "^2.2.0",
        "rimraf": "^2.6.2",
        "typescript": "^2.6.2",
        "uglify-js": "^3.2.0",
        "uglifyjs-webpack-plugin": "^1.1.1",
        "webpack": "^3.8.1",
        "webpack-dev-server": "^2.9.5",
        "custom-tslint-formatters": "^2.1.1",
        "tslint": "^5.8.0",
        "tslint-config-standard": "^7.0.0",
        "tslint-loader": "^3.5.3"
    },
    "engines": {
        "node": ">= 6.0.0",
        "npm": ">= 3.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 10"
    ],
    "license": "Apache-2.0"
}
